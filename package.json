{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "test": "jest",
    "lint": "next lint",
    "prepare": "husky install",
    "prettier-check": "prettier --check .",
    "prettier": "npx prettier . --write"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.3",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.5.4",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/mdx": "^2.0.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.288.0",
    "next": "13.5.4",
    "next-auth": "^4.23.2",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-loading-skeleton": "^3.3.1",
    "react-markdown": "^9.0.0",
    "sass": "^1.69.0",
    "sonner": "^1.0.3",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@prisma/client": "^5.4.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.5",
    "autoprefixer": "^10",
    "dotenv": "^16.3.1",
    "eslint": "^8",
    "eslint-config-next": "13.5.5",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "postcss": "^8",
    "prettier": "3.0.3",
    "prisma": "^5.4.2",
    "tailwindcss": "^3",
    "ts-jest": "^29.1.1",
    "typescript": "^5"
  }
}
